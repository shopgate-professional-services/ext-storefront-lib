{
  "id": "@shopgate-project/storefront-lib",
  "version": "1.0.0-beta.1",
  "components": [
    {
      "id": "Subscribers",
      "path": "frontend/subscriptions.js",
      "type": "subscribers"
    },
    {
      "id": "Pdp",
      "path": "frontend/portals/Pdp",
      "target": "product.description.before",
      "type": "portals"
    }
  ],
  "configuration": {
    "config": {
      "type": "admin",
      "destination": "frontend",
      "default": {
        "apiKey": null,
        "apiStage": "production",
        "useGeolocationImmediately": true,
        "saveCustomerData": "on",
        "browserHistory": true,
        "unitSystem": "metric",
        "countries": ["de"],
        "translations": {},
        "inventory": {
          "hideNumber": false,
          "showExactUntil": null,
          "showLowUntil": 5
        },
        "termsLink": null,
        "privacyLink": null,
        "displayType": "reserveButton",
        "renderLiveInventoryMode": "modal"

      },
      "params": {
        "label": "",
        "type": "json"
      }
    }
  }
}
