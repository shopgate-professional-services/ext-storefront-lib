{
  "id": "@shopgate-project/storefront-lib",
  "version": "1.0.0-beta.1",
  "components": [
    {
      "id": "Subscribers",
      "path": "frontend/subscriptions.js",
      "type": "subscribers"
    },
    {
      "id": "ClickReserveButton",
      "path": "frontend/components/ClickReserveButton",
      "target": "product.description.before",
      "type": "portals"
    }
  ],
  "configuration": {
    "config": {
      "type": "admin",
      "destination": "frontend",
      "default": {
        "apiKey": null,
        "apiStage": "production",
        "useGeolocationImmediately": true,
        "saveCustomerData": "on",
        "browserHistory": true,
        "unitSystem": "metric",
        "localization": {},
        "inventory": {
          "hideNumber": false,
          "showExactUntil": null,
          "showLowUntil": 5
        },
        "productCode": "",
        "termsLink": "",
        "privacyLink": "",
        "renderLiveInventory": false,
        "renderLiveInventoryMode": "list",
        "newsletterOptIn": "disabled"
      },
      "params": {
        "label": "Storefront configuration",
        "type": "json"
      }
    }
  }
}
